{
  "name": "tracing",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "scripts": {
    "setup": "pnpm install --unsafe-perm",
    "preinstall": "node ./scripts/preinstall.js",
    "postinstall": "husky install",
    "bootstrap": "esno ./scripts/bootstrap.ts",
    "format": "prettier --write .",
    "eslint": "eslint . --ext .ts,.js",
    "dev": "esno ./scripts/dev.ts --t",
    "build": "esno ./scripts/build.ts",
    "remove": "esno ./scripts/remove.ts",
    "release": "esno ./scripts/release.ts",
    "test:unit": "jest",
    "test:watch": "jest --coverage --watch",
    "test:coverage": "jest --coverage",
    "test:tsc": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "playground": "cd playground && pnpm run dev"
  },
  "author": "wermdany <https://github.com/wermdany>",
  "license": "MIT",
  "keywords": [
    "tracker",
    "report",
    "collect",
    "FE",
    "web",
    "browser"
  ],
  "files": [
    "index.js",
    "dist"
  ],
  "formats": [
    "esm-bundler",
    "esm-browser",
    "cjs",
    "global"
  ],
  "dependencies": {
    "@microsoft/api-extractor": "7.20.1",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.2.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "*",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chalk": "^5.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.14.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^12.3.5",
    "minimist": "^1.2.5",
    "msw": "^0.49.2",
    "ora": "^6.1.0",
    "prettier": "^2.5.1",
    "rollup": "^3.8.0",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "semver": "^7.3.5",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --parser=typescript --write",
      "eslint"
    ]
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": "^14.16.0 || ^16.11.0 || ^17.0.0"
  }
}
