{
  "extends": "../../api-extractor.json",
  "mainEntryPointFilePath": "./dist/packages/browser-tracker/src/index.d.ts",
  "dtsRollup": {
    "publicTrimmedFilePath": "./dist/browser-tracker.d.ts"
  },
  "compiler": {
    "overrideTsconfig": {
      "compilerOptions": {
        "baseUrl": "packages/browser-tracker/dist/",
        "paths": {
          "@tracker/*": ["packages/*/src"]
        }
      },
      "include": ["global.d.ts", "packages/*/src/**/*.ts"],
      "exclude": ["test-utils/*"]
    }
  }
}
